<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified" xmlns="http://www.tallink.com/hris/"
           targetNamespace="http://www.tallink.com/hris/">
    <xs:include schemaLocation="BaseEnums.xsd"/>
    <xs:complexType name="Error">
        <xs:sequence>
            <xs:element name="Message" type="xs:string"/>
            <xs:element name="Type" type="ErrorType"/>
            <xs:element name="Code" type="xs:integer"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="CompanyName">
        <xs:restriction base="xs:string">
            <xs:maxLength value="120"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="CouponPack">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="Description" type="xs:string"/>
            <xs:element name="Interval" type="xs:integer"/>
            <xs:element name="CouponPackItems" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="CouponPackItem" type="CouponPackItem" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CreateCouponPackItem">
        <xs:sequence>
            <xs:element name="Quantity" type="xs:integer"/>
            <xs:element name="CouponTypeId" type="xs:integer"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CouponPackItem">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="Quantity" type="xs:integer"/>
            <xs:element name="CouponType" type="CouponType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CouponType">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="CountryCode" type="CountryCodeEnum" minOccurs="0"/>
            <xs:element name="SeawareCouponClass" type="xs:string" minOccurs="0"/>
            <xs:element name="Description" type="xs:string" minOccurs="0"/>
            <xs:element name="PersonalUseOnly" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CardTransaction">
        <xs:sequence>
            <xs:element name="CardNumber" type="xs:string"/>
            <xs:element name="Amount" type="xs:float"/>
            <xs:element name="Designation" type="xs:string"/>
            <xs:element name="CurrencySymbol" type="xs:string"/>
            <xs:element name="BranchName" type="xs:string"/>
            <xs:element name="EurAmount" type="xs:float"/>
            <xs:element name="Date" type="xs:dateTime"/>
            <xs:element name="OperatorNo" type="xs:integer"/>
            <xs:element name="ReceiptNo" type="xs:integer"/>
            <xs:element name="TerminalNo" type="xs:integer"/>
            <xs:element name="compNo" type="xs:integer"/>
            <xs:element name="branchNo" type="xs:integer"/>
            <xs:element name="Accountable" type="xs:boolean"/>
            <xs:element name="ProfitCenterNo" type="xs:integer"/>
            <xs:element name="CardTransactionComment" type="CardTransactionComment" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CardTransactionComment">
        <xs:sequence>
            <xs:element name="CostCenter" type="xs:string"/>
            <xs:element name="Department" type="xs:string"/>
            <xs:element name="Account" type="xs:string"/>
            <xs:element name="BusinessPurpose" type="xs:string" minOccurs="1"/>
            <xs:element name="GuestList" type="xs:string" minOccurs="1"/>
            <xs:element name="IdentityId" type="xs:integer"/>
            <xs:element name="IdentityFirstName" type="xs:string"/>
            <xs:element name="IdentityLastName" type="xs:string"/>
            <xs:element name="ApprovedByIdentity" type="xs:integer" minOccurs="0"/>
            <xs:element name="ApprovalDate" type="xs:date" minOccurs="0"/>
            <xs:element name="OwnerComment" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="HRISBaseRequest" abstract="true">
        <xs:sequence>
            <xs:element name="Authentication">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="SecurityToken" type="xs:string"/>
                        <xs:element name="UserName" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="HRISBaseResponse" abstract="true">
        <xs:sequence>
            <xs:element name="Errors" minOccurs="0">
                <xs:complexType>
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:element name="Error" type="Error"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Printer">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="Ip" type="xs:string"/>
            <xs:element name="CountryCode" type="CountryCodeEnum"/>
            <xs:element name="Type" type="PrinterTypeEnum"/>
            <xs:element name="Description" type="xs:string" minOccurs="0"/>
            <xs:element name="PrinterBrandType" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DateRange">
        <xs:sequence>
            <xs:element name="From" type="xs:dateTime"/>
            <xs:element name="To" type="xs:dateTime"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="IdentityContactInfo">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="Address" type="xs:string" minOccurs="0"/>
            <xs:element name="Phone" type="xs:string" minOccurs="0"/>
            <xs:element name="CellPhone" type="xs:string" minOccurs="0"/>
            <xs:element name="ShortPhone" type="xs:string" minOccurs="0"/>
            <xs:element name="E-mail" type="xs:string" minOccurs="0"/>
            <xs:element name="ContactType" type="IdentityContactInfoType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="HRSystemReference">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="EmployeeID" type="xs:string"/>
            <xs:element name="HRSystemType" type="HRSystemTypeEnum"/>
            <xs:element name="Status" type="HRSystemReferenceStatus" default="ACTIVE"/>
            <xs:element name="CompanyCode" type="xs:string" minOccurs="0"/>
            <xs:element name="EmplStartDate" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="EmplEndDate" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="EmplRecId" type="xs:string"/>
            <xs:element name="ContractId" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CardFullLog">
        <xs:sequence>
            <xs:element name="LogDate" type="xs:dateTime"/>
            <xs:element name="ActionLogged" type="xs:string"/>
            <xs:element name="Message" type="xs:string"/>
            <xs:element name="Blacklist" type="CardBlacklistItem"/>
            <xs:element name="LogItem" type="CardLog"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CardLog">
        <xs:sequence>
            <xs:element name="Message" type="xs:string"/>
            <xs:element name="LogDate" type="xs:dateTime"/>
            <xs:element name="CardId" type="xs:integer"/>
            <xs:element name="Username" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ImportInfo">
        <xs:sequence>
            <xs:element name="ImportDate" type="xs:dateTime"/>
            <xs:element name="ImportAction" type="CardQueueActionEnum"/>
            <xs:element name="BlockType" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CardType">
        <xs:sequence>
            <xs:element name="Id" type="xs:short" minOccurs="0"/>
            <xs:element name="Name" type="xs:string"/>
            <xs:element name="Number" type="xs:integer"/>
            <xs:element name="Limit" type="xs:decimal"/>
            <xs:element name="ImageName" type="xs:string"/>
            <xs:element name="Period" type="xs:integer"/>
            <xs:element name="IsNewLayout" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="LimitForCardType">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="1"/>
            <xs:element name="CountryCode" type="CountryCodeEnum" minOccurs="0" maxOccurs="1"/>
            <xs:element name="CardTypeId" type="xs:integer" maxOccurs="1"/>
            <xs:element name="PeriodInMonths" type="xs:integer" minOccurs="1"/>
            <xs:element name="Limit" type="xs:decimal" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="BaseCard">
        <xs:sequence>
            <xs:element name="CardTypeId" type="xs:string" minOccurs="0"
                        maxOccurs="1"/>
            <xs:element name="CountryCode" type="CountryCodeEnum"
                        minOccurs="0" maxOccurs="1"/>
            <xs:element name="IdentityId" type="xs:integer" minOccurs="0"
                        maxOccurs="1"/>
            <xs:element name="HRSystemReference" type="HRSystemReference"
                        minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CardHolder">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="CardId" type="xs:integer"/>
            <xs:element name="IdentityId" type="xs:integer"/>
            <xs:element name="Owner" type="xs:boolean" default="false"/>
            <xs:element name="ValidFrom" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="ValidTo" type="xs:dateTime" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CardInfo">
        <xs:sequence>
            <xs:element name="Blacklist">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Status" type="OscarCardStatus"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="Employee">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="FirstName" type="xs:string"/>
                        <xs:element name="LastName" type="xs:string"/>
                        <xs:sequence>
                            <xs:element name="Status" type="OscarCardStatus"/>
                        </xs:sequence>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="UserNote">
        <xs:sequence>
            <xs:element name="UserName" type="xs:string"/>
            <xs:element name="Date" type="xs:dateTime"/>
            <xs:element name="Comment" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Card">
        <xs:complexContent>
            <xs:extension base="BaseCard">
                <xs:sequence>
                    <xs:element name="Id" type="xs:integer" minOccurs="0"/>
                    <xs:element name="CardNumber" type="xs:string"
                                minOccurs="0"/>
                    <xs:element name="CardName" type="xs:string" minOccurs="0"/>
                    <xs:element name="Active" type="xs:boolean" default="true"/>
                    <xs:element name="CompanyCard" type="xs:boolean"/>
                    <xs:element name="CreateDate" type="xs:dateTime"
                                minOccurs="0"/>
                    <xs:element name="LastUpdateDate" type="xs:dateTime"
                                minOccurs="0"/>
                    <xs:element name="PhysicalCards" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="PhysicalCard" type="PhysicalCard"
                                            minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="UserNotes" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="UserNote" type="UserNote" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Blacklist" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="BlacklistItem" type="CardBlacklistItem"
                                            minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="CardHolders" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="CardHolder" type="CardHolder"
                                            minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="PhysicalCard">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="ValidDate" type="xs:date" minOccurs="0"/>
            <xs:element name="PrintDate" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="Status" type="CardStatus" minOccurs="0"/>
            <xs:element name="CompanyName" type="xs:string" minOccurs="0"/>
            <xs:element name="Department" type="xs:string" minOccurs="0"/>
            <xs:element name="DepartmentName" type="xs:string" minOccurs="0"/>
            <xs:element name="IsNewLayout" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="IdentityBase">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="Active" type="xs:boolean" minOccurs="0"/>
            <xs:element name="CreateDate" type="xs:dateTime"
                        minOccurs="0"/>
            <xs:element name="LastUpdate" type="xs:dateTime"
                        minOccurs="0"/>
            <xs:element name="HRSystemReferences" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="HRSystemReference" type="HRSystemReference"
                                    minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="IdentityContactInfoList" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="IdentityContactInfo" type="IdentityContactInfo" minOccurs="0"
                                    maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="Picture" type="xs:base64Binary" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Identity">
        <xs:complexContent>
            <xs:extension base="IdentityBase">
                <xs:sequence>
                    <xs:element name="PersonId" type="xs:integer" minOccurs="0"/>
                    <xs:element name="FirstName" type="FirstName" minOccurs="0"/>
                    <xs:element name="LastName" type="LastName" minOccurs="0"/>
                    <xs:element name="ShortFirstName" type="ShortFirstName" minOccurs="0"/>
                    <xs:element name="ShortLastName" type="ShortLastName" minOccurs="0"/>
                    <xs:element name="Gender" type="Gender" minOccurs="0"/>
                    <xs:element name="BirthDate" type="xs:date" minOccurs="0"/>
                    <xs:element name="SeawareClientId" type="xs:integer" minOccurs="0"/>
                    <xs:element name="PersonalId" type="PersonalCode" minOccurs="0"/>
                    <xs:element name="Citizenship" type="CitizenshipEnum" minOccurs="0"/>
                    <xs:element name="FirstLanguage" type="LanguageEnum" minOccurs="0"/>
                    <xs:element name="JobDescription" type="xs:string" minOccurs="0"/>
                    <xs:element name="CumulativeContractStatus" type="xs:string" minOccurs="0"/>
                    <xs:element name="Positions" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="IdentitySearchPosition" type="IdentityPosition" minOccurs="0"
                                            maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="IdentityUnperson">
        <xs:complexContent>
            <xs:extension base="IdentityBase">
                <xs:sequence>
                    <xs:element name="UnpersonId" type="xs:integer" minOccurs="0"/>
                    <xs:element name="Name" type="UnpersonName" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:simpleType name="FirstName">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="45"/>
            <xs:whiteSpace value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LastName">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="45"/>
            <xs:whiteSpace value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ShortFirstName">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="20"/>
            <xs:whiteSpace value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ShortLastName">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="20"/>
            <xs:whiteSpace value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="UnpersonName">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="100"/>
            <xs:whiteSpace value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PersonalCode">
        <xs:restriction base="xs:string">
            <xs:minLength value="11"/>
            <xs:maxLength value="11"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="CardBlockType">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="Name" type="xs:string"/>
            <xs:element name="ExternalTypeId" type="xs:integer"/>
            <xs:element name="Period" type="xs:integer"/>
            <xs:element name="ManuallyAllowed" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="StaffCouponType">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer"/>
            <xs:element name="Country" type="CountryCodeEnum"/>
            <xs:element name="SeawareCouponCode" type="xs:string"/>
            <xs:element name="Description" type="xs:string"/>
            <xs:element name="PersonalUseOnly" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CardBlacklistItem">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="CardId" type="xs:integer" minOccurs="0"/>
            <xs:element name="FromDate" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="ToDate" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="CardBlockTypeId" type="xs:integer"
                        minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OscarCardQueueItem">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="Action" type="CardQueueActionEnum"
                        minOccurs="0"/>
            <xs:element name="ImportedDate" type="xs:dateTime"
                        minOccurs="0"/>
            <xs:element name="ScheduledDate" type="xs:dateTime"
                        minOccurs="0"/>
            <xs:element name="Imported" type="xs:boolean" minOccurs="0"/>
            <xs:element name="Card" type="Card" minOccurs="0"/>
            <xs:element name="BlacklistItem" type="CardBlacklistItem"
                        minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="IdentityCouponPack">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="CouponPackId" type="xs:integer"/>
            <xs:element name="IdentityId" type="xs:integer"/>
            <xs:element name="InitialGenDate" type="xs:dateTime"/>
            <xs:element name="Active" type="xs:boolean"/>
            <xs:element name="LastGenerationDate" type="xs:dateTime" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="IdentityCouponPackQueueInfoItem">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer"/>
            <xs:element name="IdentityId" type="xs:integer"/>
            <xs:element name="FirstName" type="xs:string"/>
            <xs:element name="LastName" type="xs:string"/>
            <xs:element name="CouponPack" type="CouponPack"/>
            <xs:element name="InitialGenDate" type="xs:dateTime"/>
            <xs:element name="Active" type="xs:boolean"/>
            <xs:element name="LastGenerationDate" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="GenerationDate" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="ScheduledDate" type="xs:dateTime"/>
            <xs:element name="Status" type="IdentityCouponPackQueueStatus"/>
            <xs:element name="Message" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="HrisLog">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer"/>
            <xs:element name="UserName" type="xs:string" minOccurs="0"/>
            <xs:element name="TimeStamp" type="xs:dateTime"/>
            <xs:element name="Method" type="xs:string"/>
            <xs:element name="SecurityToken" type="SecurityToken"/>
            <xs:element name="Status" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SecurityToken">
        <xs:sequence>
            <xs:element name="ApplicationName" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Coupon">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer"/>
            <xs:element name="UserDefinedCouponId" type="xs:string"/>
            <xs:element name="VacationRange" type="DateRange" minOccurs="0"/>
            <xs:element name="ApplicabilityRange" type="DateRange"/>
            <xs:element name="Active" type="xs:boolean"/>
            <xs:element name="Used" type="xs:boolean"/>
            <xs:element name="SentTo" type="xs:string" minOccurs="0"/>
            <xs:element name="CouponClass" type="xs:string"/>
            <xs:element name="Description" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="EmailDistribution">
        <xs:list itemType="EmailAddress"/>
    </xs:simpleType>
    <xs:complexType name="FileDistribution"/>
    <xs:complexType name="DirectToPrinter">
        <xs:sequence>
            <xs:element name="PrinterId" type="xs:integer"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="CouponIdList">
        <xs:list itemType="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="EmailAddress">
        <xs:restriction base="xs:string">
            <xs:pattern value="\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="Trainer">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="Active" type="xs:boolean" minOccurs="0"/>
            <xs:element name="IdentityOverview" type="IdentityOverview" minOccurs="0"/>
            <xs:element name="OfficeCode" type="OfficeCode" minOccurs="0"/>
            <xs:element name="TrainerCompany" type="TrainerCompany" minOccurs="0"/>
            <xs:element name="TrainerContact" type="TrainerContact" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TrainerCompany">
        <xs:sequence>
            <xs:element name="Id" type="xs:integer" minOccurs="0"/>
            <xs:element name="Company" type="xs:string" minOccurs="0"/>
            <xs:element name="AddressOne" type="xs:string" minOccurs="0"/>
            <xs:element name="AddressTwo" type="xs:string" minOccurs="0"/>
            <xs:element name="Zipcode" type="xs:string" minOccurs="0"/>
            <xs:element name="City" type="xs:string" minOccurs="0"/>
            <xs:element name="Country" type="xs:string" minOccurs="0"/>
            <xs:element name="Phone" type="xs:string" minOccurs="0"/>
            <xs:element name="Email" type="EmailAddress" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
